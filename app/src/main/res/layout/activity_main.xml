<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="foxtail.cc.foxtailappassignmentconstraintlayout.MainActivity">

    <TextView
        android:id="@+id/phone_number_textview"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="010 - 9962 - 8152"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        android:textAlignment="center"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="25dp"
        app:layout_constraintHorizontal_bias="0.0" />


    <TextView
        android:id="@+id/name_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이영주"
        android:textColor="@android:color/black"
        android:textSize="17sp"
        android:layout_marginLeft="130dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/phone_number_textview" />


    <TextView
        android:id="@+id/label_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="iPhone"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:textSize="17sp"
        app:layout_constraintLeft_toRightOf="@+id/name_textview"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/phone_number_textview" />


    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.19" />


    <ImageView
        android:id="@+id/dial_1_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0156"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        android:layout_marginTop="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3" />


    <ImageView
        android:id="@+id/dial_2_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0157"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        android:layout_marginTop="0dp"
        app:layout_constraintRight_toLeftOf="@+id/dial_3_img"
        android:layout_marginRight="0dp"
        app:layout_constraintLeft_toRightOf="@+id/dial_1_img"
        android:layout_marginLeft="0dp" />

    <ImageView
        android:id="@+id/dial_3_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0158"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        android:layout_marginTop="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline3"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.15" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline4"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.85" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline5"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />



    <ImageView
        android:id="@+id/dial_4_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0159"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_1_img" />


    <ImageView
        android:id="@+id/dial_5_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0160"
        app:layout_constraintRight_toLeftOf="@+id/dial_6_img"
        android:layout_marginRight="0dp"
        app:layout_constraintLeft_toRightOf="@+id/dial_4_img"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_2_img" />

    <ImageView
        android:id="@+id/dial_6_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0161"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_3_img" />



    <ImageView
        android:id="@+id/dial_7_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0162"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_4_img" />


    <ImageView
        android:id="@+id/dial_8_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0163"
        app:layout_constraintRight_toLeftOf="@+id/dial_9_img"
        android:layout_marginRight="0dp"
        app:layout_constraintLeft_toRightOf="@+id/dial_7_img"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_5_img" />

    <ImageView
        android:id="@+id/dial_9_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0164"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_6_img" />


    <ImageView
        android:id="@+id/dial_11_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0155"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_7_img" />


    <ImageView
        android:id="@+id/dial_0_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0165"
        app:layout_constraintRight_toLeftOf="@+id/dial_9_img"
        android:layout_marginRight="0dp"
        app:layout_constraintLeft_toRightOf="@+id/dial_7_img"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_8_img" />

    <ImageView
        android:id="@+id/dial_12_img"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/img_0166"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_9_img" />

    <ImageView
        android:id="@+id/phone_call_img"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:src="@drawable/img_0146"
        app:layout_constraintTop_toBottomOf="@+id/dial_0_img"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginTop="-9dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintVertical_bias="0.625" />

    <ImageView
        android:id="@+id/dial_delete_img"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:src="@drawable/img_0147"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginRight="0dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/dial_12_img"
        app:layout_constraintLeft_toRightOf="@+id/phone_call_img"
        android:layout_marginLeft="0dp"
        app:layout_constraintHorizontal_bias="0.757"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/nav_1_img"
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:src="@drawable/img_0148"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginLeft="0dp" />

    <ImageView
        android:id="@+id/nav_2_img"
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:src="@drawable/img_0149"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/nav_1_img"
        android:layout_marginLeft="20dp" />

    <ImageView
        android:id="@+id/nav_3_img"
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:src="@drawable/img_0150"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/nav_2_img"
        android:layout_marginLeft="20dp" />

    <ImageView
        android:id="@+id/nav_4_img"
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:src="@drawable/img_0151"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/nav_3_img"
        android:layout_marginLeft="20dp" />

    <ImageView
        android:id="@+id/nav_5_img"
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:src="@drawable/img_0152"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/nav_4_img"
        android:layout_marginLeft="20dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

</android.support.constraint.ConstraintLayout>
